<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chiragmadan.mycompanion">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity" />
        <activity
            android:name=".leaveEntry"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".FeedbackPage"
            android:parentActivityName=".HomeActivity" />
        <activity android:name=".MessBillPage" />
        <activity android:name=".MessMenuPage" />
        <activity android:name=".GuestPage" />
        <activity android:name=".AdminHomePage" />
        <activity android:name=".AdminLeaveEntryPage" />
        <activity android:name=".AdminFeedbackPage"></activity>
    </application>

</manifest>